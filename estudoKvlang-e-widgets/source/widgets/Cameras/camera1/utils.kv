<Botao@Button>:
    size_hint: 1, 1 #x, y

<Box@BoxLayout>:
    orientation: 'vertical'
    padding: 10
    spacing: 10

<Pilha@StackLayout>:
    orientation: "tb-rl"
    padding: 10
    spacing: 10

<EntradaTexto@TextInput>:
    multiline: False
    size_hint: 1., .09
    width: root.width
    height: root.height